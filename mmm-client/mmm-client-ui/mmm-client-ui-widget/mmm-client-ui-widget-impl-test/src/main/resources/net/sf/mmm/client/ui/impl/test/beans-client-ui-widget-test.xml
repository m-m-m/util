<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xmlns:context="http://www.springframework.org/schema/context"
  xsi:schemaLocation="http://www.springframework.org/schema/beans
                      http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
                      http://www.springframework.org/schema/context
                      http://www.springframework.org/schema/context/spring-context-3.0.xsd">

  <import resource="classpath:net/sf/mmm/util/beans-util-core.xml" />

  <context:component-scan base-package="net.sf.mmm.client.ui.impl.test"/>

  <bean class="net.sf.mmm.client.ui.base.aria.role.RoleFactoryImpl"/>
  <bean class="net.sf.mmm.client.ui.base.UiAccessKeyBindingImpl"/>
  <bean class="net.sf.mmm.client.ui.base.binding.UiDataBindingFactoryImpl"/>
  <bean class="net.sf.mmm.client.ui.base.UiConfigurationDefault"/>
  <bean class="net.sf.mmm.client.ui.base.UiModeChangerImpl"/>
  <bean class="net.sf.mmm.client.ui.base.widget.factory.UiWidgetFactoryImpl"/>

</beans>